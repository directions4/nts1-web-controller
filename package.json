  "devDependencies": {
    "@quasar/vite-plugin": "^1.4.0",
    "@vitejs/plugin-vue": "^4.2.0",
    "@vue/test-utils": "^2.4.0",
    "babel-eslint": "^10.1.0",
    "browser-sync": "^2.29.0",
    "cpx": "^1.5.0",
    "eslint": "^8.44.0",
